{
  "Name" : "Orthanc Configuration",
  "RemoteAccessAllowed" : true,
  "AuthenticationEnabled" : true,
  "RegisteredUsers" : {
    "eran" : "langtOg Godt"
  },
  "DicomServerEnabled" : true,
  "DicomAET" : "ORTHANC",
