{
  "Name" : "Orthanc Configuration",
  "RemoteAccessAllowed" : true,
  "AuthenticationEnabled" : true,
  "RegisteredUsers" : {
    "eran" : "langtOg Godt"
  },
  "DicomServerEnabled" : true,
  "DicomAET" : "ORTHANC",
  "DicomCheckCalledAet" : false,
  "DicomAlwaysAllowStore" : true,
  "DicomAlwaysAllowEcho" : true,
  "DicomAlwaysAllowGet" : false,
  "DicomCheckModalityHost" : false,
  "SslEnabled" : false,
  "MetricsEnables" : true,
  "DicomTlsEnabled" : false,
  "DicomTlsRemoteCertificateRequired" : false,
  "DicomModalities" : {
    "konsoll" : [ "konsoll", "konsoll.medtek.haukeland.no", 104 ],
    "orthanc" : [ "ORTHANC", "127.0.0.1", 4242 ]
  },
  "PostgreSQL" : {
    "EnableIndex" : true,
    "EnableStorage" : true,
    "Host" : "${DB_ADDR}",
    "Port" : ${DB_PORT},
    "Database" : "orthancdb",
    "Username" : "${DB_USERNAME}",
    "Password" : "${DB_PASSWORD}",
    "EnableSsl" : false,
    "Lock" : false
  },
  "Plugins" : [
    "/usr/local/share/orthanc/plugins/libOrthancPostgreSQLIndex.so",
    "/usr/local/share/orthanc/plugins/libOrthancPostgreSQLStorage.so",
    "/usr/local/share/orthanc/plugins/libOrthancWebViewer.so",
    "/usr/local/share/orthanc/plugins/libOrthancPython.so"
  ],
  "PythonScript" : "/tmp/orthanc/process.py",
  "StableAge": 60
}
